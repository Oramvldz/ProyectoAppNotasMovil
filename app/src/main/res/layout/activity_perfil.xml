<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PerfilActivity">

    <RelativeLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:background="#D6F3FF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <RelativeLayout
            android:layout_width="347dp"
            android:layout_height="365dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="31dp"
            android:layout_marginTop="176dp"
            android:layout_marginEnd="31dp"
            android:layout_marginBottom="188dp"
            android:background="#FFFFFF">

            <TextView
                android:id="@+id/lblNombres"
                android:layout_width="101dp"
                android:layout_height="46dp"
                android:layout_above="@+id/lblApellidos"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="22dp"
                android:layout_marginTop="71dp"
                android:layout_marginEnd="24dp"
                android:layout_marginBottom="61dp"
                android:layout_toStartOf="@+id/txtNombres"
                android:text="Nombres" />

            <TextView
                android:id="@+id/lblApellidos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/lblCorreo"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginBottom="62dp"
                android:text="Apellidos" />

            <TextView
                android:id="@+id/lblCorreo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="21dp"
                android:layout_marginEnd="212dp"
                android:layout_marginBottom="114dp"
                android:text="Correo" />

            <EditText
                android:id="@+id/txtNombres"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="49dp"
                android:layout_marginEnd="13dp"
                android:layout_marginBottom="265dp"
                android:ems="10"
                android:inputType="text" />

            <EditText
                android:id="@+id/txtApellidos"
                android:layout_width="wrap_content"
                android:layout_height="51dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="52dp"
                android:layout_marginTop="136dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="178dp"
                android:layout_toEndOf="@+id/lblApellidos"
                android:ems="10"
                android:inputType="text" />

            <EditText
                android:id="@+id/txtCorreo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="131dp"
                android:layout_marginTop="216dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="104dp"
                android:ems="10"
                android:inputType="text" />

            <Button
                android:id="@+id/btnCambiarEmail"
                android:layout_width="133dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="28dp"
                android:layout_marginTop="284dp"
                android:layout_marginEnd="186dp"
                android:layout_marginBottom="23dp"
                android:text="Cambiar Email" />

            <Button
                android:id="@+id/btnCambiarContrasenia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="195dp"
                android:layout_marginTop="279dp"
                android:layout_marginEnd="29dp"
                android:layout_marginBottom="22dp"
                android:text="Cambiar ContraseÃ±a" />
        </RelativeLayout>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>